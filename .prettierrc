{
  "plugins": [
    "@shopify/prettier-plugin-liquid",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": ["*.liquid", "*.html"],
      "options": {
        "parser": "liquid-html"
      }
    }
  ],
  "printWidth": 80,
  "singleQuote": false,
  "tabWidth": 2,
  "semi": false
}
