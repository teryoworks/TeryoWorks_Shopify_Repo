{
  "plugins": ["@shopify/prettier-plugin-liquid", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": ["*.liquid", "*.html"],
      "options": {
        "parser": "liquid-html",
        "htmlWhitespaceSensitivity": "ignore",
        "proseWrap": "preserve",
        "embeddedLanguageFormatting": "auto"
      }
    }
  ],
  "printWidth": 120,
  "singleQuote": false,
  "tabWidth": 2,
  "semi": false,
  "endOfLine": "auto"
}
